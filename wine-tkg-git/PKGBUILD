# vkd3d deps
if [ "$_use_vkd3d" == "true" ]; then
  makedepends+=('vkd3d' 'lib32-vkd3d')
fi

# proton-tkg deps
if [ -e "$_where"/proton_tkg_token ]; then
  makedepends+=('fontforge' 'python-fonttools')
  depends+=('tk')
fi

# wine-mono deps
if [ "$_use_mono" == "true" ]; then
  depends+=('wine-mono')
fi

optdepends=(
    'giflib'                'lib32-giflib'
    'libpng'                'lib32-libpng'
